    public static void ArrivalTime(String[] args){
        Scanner input = new Scanner(System.in);
        int hour, min, time=2;
        String mins = "";
        String hrs = "";
        System.out.print("What hour does she leave? (military) ");
        hour = input.nextInt();
        System.out.print("What minute does she leave, choose 1 for on the hour, 2 for 20, 3 for 40? ");
        min = 20*(input.nextInt()-1);
        
        if ((hour>=7&&hour<10)||(hour>=15&&hour<19)){
            time = 4;
        }
        if (min ==0){
           mins ="0";
        }
        if ((hour +time) > 24){
            hour = hour%24;
        }
        if (hour < 7 && (hour +time) > 24){
            hrs = "0";
        }
        System.out.print("She will arrive at ");
        System.out.println(hrs+(hour+time) + ":" + min + mins);
    }
    public static void VoteCount(String [] args){
        int points[] = new int[2];
        int total;
        String votes;
        String win ;
        Scanner input = new Scanner(System.in);
        
        System.out.print("Number of votes: ");
        total =  input.nextInt();
        input.nextLine();
        do {
        System.out.print("Enter the Sequence of votes(ABBABBA)");
        votes = input.nextLine();
        } while (votes.length() != total);
        
        for (int i =0; i < total; i++){
            
            if ((votes.charAt(i))=='A'){
                points[0]++;
            }
            else if ((votes.charAt(i))=='B'){
                points[1]++;
            }
        }
        if (points[0]>points[1]){
            win = "A";
            System.out.println("A wins! With this many points " + points[0] );
        }
        else if(points[1]>points[0]){
            win = "B";
            System.out.println("B wins! With this many points " + points[1]);
        }
        else if(points[1]==points[0]){
           win = "T";
           System.out.println("A Tie!! At this many points " + points[0]);
        }
        
        
        
    }
    public static void Telemarketer(String[] args){
        Scanner input = new Scanner(System.in);
        boolean val = false;
        char l4d[] = new char[4];
        System.out.print("Enter a phone number: ");
        String phonenumb = input.nextLine();
        for (int i = 0; i<4; i++){
            l4d[i] = phonenumb.charAt(i+3);
        }
        if ((l4d[0] == '8' || l4d[0] =='9') && (l4d[1]==l4d[2])&&(l4d[3]=='8'||l4d[3]=='9')){
            val = true;
        }
        System.out.println("It is " + val + " that the number is a telemarketer.");
    }
    public static void Speedingfines(String[] args){
        int speed, speelim;
        Scanner input = new Scanner(System.in);
        System.out.print("Enter your speed: ");
        speed = input.nextInt();
        System.out.print("Enter the limit: ");
        speelim = input.nextInt();
         int diff = speed - speelim;
         if (diff<=0){
            System.out.print("You are obeying the Law"); 
         }
         else if (diff>=1&&diff<=20){
            System.out.println("You are speeding, Fine is $100 "); 
         }
         else if (diff>20&&diff<=30){
            System.out.println("You are speefding, Fine is $270"); 
         }
         else if (diff>30){
            System.out.println("You are speeding, Fine is $500 "); 
         }
    }
    public static void FishFinder(String[] args){
        Scanner input = new Scanner(System.in);
        int depth[] = new int[4];
        int diff[] = new int[3];
        int count =0;
        for(int i = 0; i<4; i++){
            System.out.print("Enter the fish depth position " + (i+1) + ": ");
            depth[i] = input.nextInt();
        }
        for (int i =0; i<3;i++){
            diff[i]=depth[i+1]-depth[i];
            if (diff[i]<0){
                count --;
            }
            else if (diff[i]>0){
                count++;
            }
        }
        
        if (count ==-3){
            System.out.println("Fish diving");
        }
        else if (count ==3){
            System.out.println("Fish rising");
        }
        else {
            System.out.println("No fish");
        }
        
    }
     public static void IconScaling(String[] args){
        int sf;
        
        Scanner input = new Scanner(System.in);
        do{
        System.out.print("Enter the scaling factor: ");
        sf = input.nextInt();
        } while (sf <0 ||sf>25);
        input.nextLine();
        String lines[] = new String[3];
        char arr[][] = new char[3][3];
        for (int i =0; i < 3; i++){
            System.out.println("Enter the line in position " + (i+1));
            lines[i] = input.nextLine(); 
            arr[i] = lines[i].toCharArray();
        }
        for (int i =0; i <3; i++){
          for (int f=0; f<sf;f++) { 
            for (int j =0; j<3;j++){
                for (int c =0; c<sf;c++){
                    System.out.print(arr[i][j]);
                }
            }
            System.out.println();
          }
        }
       
    }
    public static void Sheldonandleonard(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.print("Enter the coded word: ");
        String coded = input.nextLine();
        char codedarr[] = coded.toCharArray();
        char decoded[] = new char[coded.length()];
        String alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
        char alpha[] = alphabet.toCharArray();
        
        for (int i = 0; i<coded.length();i++){
            for (int j =0; j<26; j++){
                if (codedarr[i] == alpha[j]){
                    decoded[i] = alpha[((j+3)%26)];
                }
            }
        }
        
        System.out.println("The decoded message is: " );
        
        for(int i = 0;i<coded.length();i++){
            System.out.print(decoded[i]);
        }
        
        System.out.println();
    }
