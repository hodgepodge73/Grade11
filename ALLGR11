    public static void Tutorial(String[] args){
        
        int a = 0;
        
        while (a<10){
            
            System.out.println(a);
            a++; // adds one each time
        }
    
        System.out.println("-----------------------");
        
        int b=0;
        do {
            System.out.println(b);
            b++;
        } while (b<10);
        
    
    
        
     }
    public static void prompter(String[] args){
        
        int min, max, guess;
        
        Scanner input = new java.util.Scanner(System.in);
        
        
        System.out.print("Pick a minimum number: ");
        min = input.nextInt();
        System.out.print("Pick a maximum number: ");
        max = input.nextInt();
        
        System.out.print("Pick a number *within* the range: ");
        guess = input.nextInt();
        
        while (guess <= min || guess >= max){
            System.out.print("Try again: ");
            guess = input.nextInt();
        
        }
        
        System.out.println("You got it in the range!");
    }
    public static void percentpassing(String[] args){
        Scanner p = new Scanner(System.in);
        
        double overfifty=0, grade, total=0, count= 0;
        
        System.out.print("Enter your first grade (enter -1 to quit): ");
        grade = p.nextDouble();
        while (grade<0){
            System.out.print("Try again (enter -1 to quit): ");
            grade = p.nextDouble();
        }
        while (grade != -1){
            if (grade>=0){
                if (grade>=50){
                    overfifty ++;
                }
                total += grade;
                System.out.print("Enter another grade (-1 to quit): ");
                grade = p.nextDouble();
                count ++;
            }
            
        }
        
        System.out.println("Your average is: " + (Math.round(total/count*100)/100.0) + "\n" + "The amount of passing grades are: " + overfifty + ", An average of: " +  (Math.round(100*overfifty/count*100)/100.0) + "% of them passing.");
    
    }
    public static void NumSum(String[] args){
        
        int bignum, i=1;
        
        Scanner input = new java.util.Scanner(System.in);
        
        System.out.print("Pick an integer: ");
        bignum = input.nextInt();
        
        if (bignum>0){    
            
            if (bignum>50){
                while (i<=bignum) {
                    System.out.print(i + " ");  
                    i++;
                }
            }

            else {    
                while (i<=bignum) {
                    System.out.println(i);  
                    i++;
                }
            }
        }
        else{
            bignum = -1*bignum;
            while (i<=bignum) {
                    System.out.println("-" + i);  
                    i++;
            }
        }
        
            
        System.out.println("The absolute sum is: " + (bignum*(bignum+1)/2));
        
        
    }
