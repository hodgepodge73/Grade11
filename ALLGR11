    public static void Strings(String[] args){
        
        String pword, username;
        Scanner input = new Scanner(System.in);
        
        System.out.print("Choose a username: ");
        username = input.nextLine();
    
        do{
        System.out.print("Choose an 8 character password: ");
        pword = input.nextLine();
        } while ((pword.length())<8);
        
        System.out.println("Your username is: " + username.toLowerCase() + "\nYour password is: " + pword.toLowerCase());
        
        
    }
    public static void monogram(String[] args){
        
        String fn,mn,lan, mon;
        
        Scanner input = new Scanner(System.in);
        
        System.out.print("First name: ");
        fn = input.nextLine();
        System.out.print("Middle name initial: ");
        mn = input.nextLine();
        System.out.print("Last name: ");
        lan = input.nextLine();
        
        char fin, min, lasn;
        
        fin = (fn.toLowerCase()).charAt(0);
        min = (mn.toLowerCase()).charAt(0);
        lasn = (lan.toUpperCase()).charAt(0);
        
        System.out.println("Your monogram is: " + fin + min + lasn);
    }
    public static void SWITCH(String[] args){
        
        // switch statement
        
        int month;
        
        Scanner input = new Scanner(System.in);
        
        System.out.println("Enter your birth month number");
        
        month = input.nextInt();
        
        switch (month){
            case 1: System.out.println("January"); break;
            case 2: System.out.println("February"); break;
            case 3: System.out.println("March"); break;
            case 4: System.out.println("April"); break;
            default: System.out.println("Invalid Input"); break;
        }
    }
