     public static void Birthday(String[] args){
        System.out.println("Lets play a game: ");
        System.out.println("1. Determine your birth month (January = 1, February = 2 and so on)");
        System.out.println("2. Multiply that number by 5.");
        System.out.println("3. Add 6 to that number.");
        System.out.println("4. Multiply the number by 4.");
        System.out.println("5. Add 9 to the number.");
        System.out.println("6. Multiply that number by 5.");
        System.out.println("7. Add your birth day to the number (10 if born on the 10th and so on.)");
        //Just outputing the game
        
        
        int Bval, newVal, month, day;
        //initializing the variables 
        
        Scanner input = new  Scanner(System.in);
        //getting the scanner ready
        System.out.print("What value did you get? ");
        Bval = input.nextInt();
        //taking user input
        newVal = (Bval-165);
        //newVal used to store the birthday as 3 digit int XYZ
        month = newVal/100;
        //determining the month by taking and int/100 to get the first of the 3 numbers
        day = newVal-month*100;
        //determining day by taking XYZ-the value in the hundreds column
        System.out.println("Your birthday is: " + month + "/" + day);
        //outputting it
        
    }
    
    public static void Asiignment(String[] args){
        
        
        System.out.println(" # of pennies?");
        Scanner scan=new Scanner(System.in);
        int numberofPennies=scan.nextInt();
        System.out.println("# of Nickels?\n");
        int numberofNickels=scan.nextInt();
        
        System.out.println("She has " + numberofPennies + " pennies");
        System.out.println("She has " + numberofNickels + " nickles");
        
    
        float totalValue = (float) (numberofPennies+5*numberofNickels)/100;
        
        System.out.println("Total $ = " + totalValue);
        
        System.out.println();
        float fifties, twenties, tens, fives, toonies, dollars, quarters, dimes, nickels, newValue;
        
        fifties = (totalValue-totalValue%50)/50;
        newValue = totalValue%50; //good to know $$ left
        twenties = (newValue-(newValue)%20)/20;
        newValue = newValue%20;
        tens = (newValue-(newValue)%10)/10;
        newValue = newValue%10;
        fives = (newValue-(newValue)%5)/5;
        newValue = newValue%5;
        toonies = (newValue-(newValue)%2)/2;
        newValue = newValue%2;
        dollars = (newValue-(newValue)%1)/1;
        newValue = newValue%1;
        quarters = (float) ((newValue-(newValue)%0.25)/0.25);
        newValue = (float) (newValue%0.25);
        dimes = (float) ((newValue-(newValue)%0.1)/0.1);
        newValue = (float) (newValue%0.1);
        nickels = (float) ((newValue-(newValue)%0.05)/0.05);
        float pennies = (float) ((newValue%0.05)/0.01);
        pennies = java.lang.Math.round(pennies);
       
        /* if ((totalValue-(fifties+twenties+tens+fives+dollars+quarters+dimes+nickels+pennies)) > 0) {
        
        pennies = pennies + 1;
    } */
        
        System.out.println(fifties + ": 50 dollar bills \n" + twenties + ": 20 dollar bills \n"
        + tens + ": Ten dollar bills \n" + fives + ": Five dollar bills \n" + toonies + ":toonies \n" +
                dollars + ": Dollars\n" + quarters + ": Quarters \n" + dimes + ": dimes\n"
                + nickels + ": Nickels\n" + pennies + ": Pennies\n");
        
             
    }
    public static void BasicArithmetic(String[] args){
        
        double x, y, answer, secondAnswer, thirdAnswer;
        
        x = 2.0;
        y = 7.0;
        answer = x + y/x;
        secondAnswer = ((x*y)-(y/3))/4;
        thirdAnswer = y%x;
        
        System.out.println("Answer = " + answer);
        System.out.println("Answer = " + secondAnswer);
        System.out.println("Answer = " + thirdAnswer);
        
        System.out.println("x = " + x);
        
        x = x + 2;
        
        System.out.println("x = " + x);
        
        x = x + 2;
        
        System.out.println("x = " + x);
           
               
    }
