            System.out.println("num1 in while:" + num1);  //debug
            if (num1 % 3 == 0) {
		num2 += num1;
                System.out.println("num2:" + num2);	       //debug
		System.out.print(num2 + " ");
	  }
	  num1 += 1;
	}

    }
