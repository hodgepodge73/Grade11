    public static void CIRCUMFERENCE(String[] args){
        
        double rad;
        Scanner input = new Scanner(System.in);
        
        System.out.print("Give me your radius: ");
        rad = input.nextDouble();
        
        if (rad>=0){
            double circumference = rad*2*(Math.PI);
            circumference = Math.round(circumference*100)/100.0;
            System.out.println("Here is your circumference: " + circumference + "cm");
            double area = (rad*rad)*(Math.PI);
            area = Math.round(area*100)/100.0;
            System.out.println("Here is your area: " + area + "cm^2");
        }
        else {
            System.out.println("I cant use a negative number, try again please");
        }
    }
    public static void spending(String[] args){
        Scanner input = new Scanner(System.in);
        
        double food, clothing, entertainment, rent;
        
        System.out.println("Enter the amount spent last month on the following items: "); 
        System.out.print(" Food Cost? $"); 
        food = input.nextDouble();
        
        System.out.print(" Clothing Cost? $"); 
        clothing = input.nextDouble();
        
        System.out.print(" Entertainment Cost? $"); 
        entertainment = input.nextDouble();
       
        System.out.print(" Rent? $"); 
        rent = input.nextDouble();
        System.out.println(); 
        
        double foodp, clothingp, entertainmentp, rentp;
        
        double total = food + clothing + entertainment + rent;
        
        foodp = 100*food/total;
        clothingp = 100*clothing/total;
        entertainmentp = 100*entertainment/total;
        rentp = 100*rent/total;
        

        foodp = Math.round(foodp*100)/100.0;
        clothingp = Math.round(clothingp*100)/100.0;
        entertainmentp = Math.round(entertainmentp*100)/100.0;
        rentp = Math.round(rentp*100)/100.0;
       
        
        System.out.println("Category            Budget"); 
       
        System.out.println("Food                " + foodp + "%"); 
        
        System.out.println("Clothing            " + clothingp + "%"); 
        
        System.out.println("Entertainment       " + entertainmentp + "%"); 
        
        System.out.println("Rent                " + rentp + "%"); 
        
    }
    public static void ROUNDING(String[] args){
        
        int integer1= 5;
        double decimal1 = 1.5;
        
        //Type Casting: converts a number  of one type to a number of a different, but compatible type
        
        int newDecimal = (int) decimal1;
        
        System.out.println("Here is my decimal: " + decimal1);
        System.out.println("Here is the integer turned to a decimal " + newDecimal);
        
        double newInteger = (double) integer1;
               
        System.out.println("Here is my integer " + integer1);
        System.out.println("Here is the integer turned to a decimal " + newInteger);
        
        // Rounding -  there is a way to use math and cheat the rrounding of numbers
        //you know we would follow the rules of rounfing
        //we can trick the comp into rounding by adding 0.5
        
        double newNum=2.6;
        int roundedNum = (int)(newNum+0.5);
        
        
        System.out.println("Here is our decimal: " + newNum);
        System.out.println("Here is our rounded number " + roundedNum);
        System.out.println();
        
        /* what about to 2 decimal places ex 12.377 to 12.38
        we can modify previous to make it work
        */
        
        double numTwo = 12.377;
        //move over decimal 2 places
        System.out.println("We started with: " + numTwo);
        numTwo = 100*numTwo; //1237.7
        
        //add 0.5 and keep the int portion(like befire)
        
        int newNumTwo = (int)(numTwo + 0.5); //1238
        System.out.println("Then we got: " + newNumTwo);
        
        // then move decimal back
        
        double roundedNumTwo = (double)(newNumTwo);
        roundedNumTwo = roundedNumTwo/100;
    }
    
    public static void MOREROUND(String[] args){
        
        Scanner input = new Scanner(System.in);
        
        double item;
        double withTax;
        
        System.out.println("Enter the price of your item: ");
        System.out.print("Item: $");
        item = input.nextDouble();
        
        withTax= item*1.13;
        
        System.out.println("This is the total cost with tax not rounded: $" + withTax);
        //round to a whole number
        
        //this will move the decimal place over and the decimal place over and then round off the remaining
        
       //then divide by 100 to move it back
       
       withTax = Math.round(withTax*100)/100.0;
       
       System.out.println("This is the total cost of with the tax rounded: $" + withTax);
    }
    
    public static void RANDOMNUMBER(String[] args){
        
        // generate random numb between 0 and 1
        
        double num = Math.random();
        System.out.println("The random number is: " + num);
        
        // 1 to 10
        
        double numTwo = 10*Math.random() + 1;
        System.out.println("The random number is: " + numTwo);
        
        //int 1-10
        int numThree = (int)(10*Math.random() + 1);
        System.out.println("The random number is: " + numThree);
        
        // formula range: (high-low +1)*Math.random() +lowest;
        
        //absolute value
        int a = Math.abs(-4);
        System.out.println("The absolute value of -4 is: " + a);
        
        //powers or exponents
        
        int b = (int) Math.pow(4, 5);
        System.out.println("4 to the 5th: " + b);
        
        double c = Math.pow(4.5, 5);
        System.out.println("4.5 to the 5th: " + c);
        
        //root
        int d = 16;
        int f = (int) Math.sqrt(d);
        System.out.println("The root of 16 is: " + f);
    }
    public static void QUADRATICFORMULA(String[] args){
        
        
        
        double a, b, c;
        Scanner input = new java.util.Scanner(System.in); 
        
        System.out.print("Enter the a value: ");
        a = input.nextDouble();
        System.out.print("Enter the b value: ");
        b = input.nextDouble();
        System.out.print("Enter the c value: ");
        c = input.nextDouble();
        
        //check
        double rootpl, rootmin;
        double check = (b*b) - (4*a*c);
        
        rootpl = (-b + Math.sqrt(Math.pow(b, 2)-(4*a*c)))/(2*a);
        rootmin = (-b - Math.sqrt(Math.pow(b, 2)-(4*a*c)))/(2*a);
        
        rootpl = Math.round(rootpl*100)/100.0;
        rootmin = Math.round(rootmin*100)/100.0;
        
        if (check<0){
            System.out.println("No real roots! But the imaginary roots are: ");
            rootpl = (-b + Math.sqrt(-(Math.pow(b, 2)-(4*a*c))))/(2*a);
            rootmin = (-b - Math.sqrt(-(Math.pow(b, 2)-(4*a*c))))/(2*a);
            
            rootpl = Math.round(rootpl*100)/100.0;
            rootmin = Math.round(rootmin*100)/100.0;

            System.out.println(rootpl + "i and " + rootmin + "i");
        }
        else if (check>0){
            
            if (rootpl<rootmin){
                System.out.println("The roots are " + rootpl + " and " + rootmin);
              
            }
            else {
                System.out.println("The roots are " + rootmin + " and " + rootpl);
              
            }
        }
        else {
            System.out.println("The one zero is: " + rootpl);
        }
   
    }
    public static void MATHTUTOR(String[] args){
        
        int op = (int)(5*Math.random() + 1);
        int int1 = (int)(10*Math.random() + 1);
        int int2 =(int)(10*Math.random() + 1);
        double answer=0, user;
        int i = 0;
       
        
        switch(op){
            case 1: answer = int1*int2; System.out.print("What is the value of " + int1 + "*" + int2 + "? "); break;
            case 2: answer = int1+int2; System.out.print("What is the value of " + int1 + "+" + int2 + "? "); break;
            case 3: answer = int1-int2; System.out.print("What is the value of " + int1 + "-" + int2 + "? "); break;
            case 4: answer = (double)int1/int2; System.out.print("What is the value of " + int1 + "/" + int2 + "? "); break;
            case 5: answer = Math.pow(int1, int2); System.out.print("What is the value of " +int1 + "^" + int2 + "? ");break;
            
        }
        
        answer = Math.round(answer*100)/100.0;
        
        while (i<2){    
            Scanner input = new java.util.Scanner(System.in); 
        
            user = input.nextDouble();
        
            if (answer==user){
                System.out.println("Correct!!!");
                break;
            }
            else {
                System.out.print("Wrong!! ");
                if (i==1){
                    System.out.println("The answer is: " + answer);
                }
                else {
                    System.out.print("Try again: ");
                }
            }
        i++;
        }
        
       
    }
    public static void TORNADO(String[] args){
        
         
        int cat;
        
        Scanner input = new Scanner(System.in);
        
        System.out.println("Enter the category of the hurricane");
        
        cat = input.nextInt();
        
        switch (cat){
            case 1: System.out.println("Category 1: 74 – 95 mph or 64-82 kt or 119-1553 km/h"); break;
            case 2: System.out.println("Category 2: 96-110 mph or 83-95 kt or 154-177 km/h"); break;
            case 3: System.out.println("Category 3: 111 – 130 mph or 96-113 kt or 178-209 km/h"); break;
            case 4: System.out.println("Category 4: 131-155 mph or 114-135 kt or 210-249 km/h"); break;
            case 5: System.out.println("Category 5: greater than 155 mph or 135 kt or 249 km/h"); break;
            default: System.out.println("WOAH, THATS WAY OFF THE SCALE");
        }
        
    }
    
    public static void GUESSINGGAME(String[] args){
        
        int secret = (int)(20*Math.random() + 1);
        
        int guess = 0;
        
        int i = 0;
        while (i<2){
            Scanner input = new java.util.Scanner(System.in);
            System.out.print("Try to guess my secret number from 1-20: ");
            guess = input.nextInt();
        
        
            if (guess == secret){
                System.out.println("Correct!!!");
                break;
            }
            else if (guess < secret && guess >= 1){
                System.out.println("Too low! Try one more time!");
                
            }
            else if ( guess > secret && guess <= 20){
                System.out.println("Too high! Try one more time!");
            }
            else{
                System.out.println("Out of range!!! Try again.");
            }
            i++;
        
        }
        System.out.println("\nHere is your last guess: " + guess);
        System.out.println("Here is my secret number: " + secret);
            if (guess != secret){
                System.out.println("Better luck next time");
            }
            else{
             System.out.println("You Are a Winner!!!");
            }
        }
                
