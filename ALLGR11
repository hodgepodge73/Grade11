    public static void FOODORDEER(String[] args){
        
        double burger_price = 3.69, fry_price=1.99, soda_price=0.99;
        
        int numburgers, numfry, numsoda;
        
        Scanner input = new  Scanner(System.in);
        
        System.out.print("Enter the number of burgers: ");
        numburgers = input.nextInt();
        System.out.print("Enter the number of fries: ");
        numfry = input.nextInt();
        System.out.print("Enter the number of sodas: ");
        numsoda = input.nextInt();
       
        double subtotal = burger_price*numburgers + fry_price*numfry + numsoda*soda_price;
        
        double tax = subtotal*0.13;
        
        double fin = subtotal + tax;
        
        subtotal = Math.round(subtotal*100)/100.0;
          
        fin = Math.round(fin*100)/100.0;
        
        tax = Math.round(tax*100)/100.0;
        
        System.out.println(" Subtotal before tax: " + subtotal + "\n Tax: " + tax
        + "\n Final Total: " + fin);
        
        System.out.print("\n Amount tendered: ");
        double cash = input.nextDouble();
        
        System.out.println(" Change: " + (cash-fin));
    }
    public static void TimeConversion(String[] args){
        
        Scanner input = new  Scanner(System.in);
        int mins;
        
        System.out.print("Enter the time in minutes: ");
        mins = input.nextInt();
        
        int hr = mins/60;
        int minsslot = mins % 60;
        if (hr > 24) {
            hr = hr%24;
        }
        if (minsslot < 10){
            System.out.println("The time is: " + hr + ":0" + minsslot);
        }
        else {
        System.out.println("The time is: " + hr + ":" + minsslot);
        }
        
    }
