    public static void TicTacToe(String[] args){
        
                
        System.out.format("%9s%9s", "|", "|\n");
        System.out.format("%9s%9s", "|", "|\n");
        System.out.format("%9s%9s", "|", "|\n");            
        System.out.print("--------------------------\n");
        System.out.format("%9s%9s", "|", "|\n");    
        System.out.format("%9s%4.5s%5s", "|", "x", "|\n");
        System.out.format("%9s%9s", "|", "|\n");
        System.out.print("--------------------------\n");
        System.out.format("%9s%9s", "|", "|\n");
        System.out.format("%9s%9s", "|", "|\n");
        System.out.format("%9s%9s", "|", "|\n");
        System.out.println();
        System.out.println("Tic Tac Toe Rules");
        System.out.println("Tic Tac Toe is a game for 2 players.");
        System.out.println("1. A grid of 9 squares is drawn.");
        System.out.println("2. The first player draws an x in any empty square.");
        System.out.println("3. The second player draws an O in any empty square.");
        System.out.println("4. Steps 2 and 3 are repeated until a Tic Tac Pattern is created:\n" +
"   Patterns can be diagonal, horizontal, or vertical.");
        System.out.println("5. A winning pattern is completed by drawing a line through the pattern. ");
        
    }
