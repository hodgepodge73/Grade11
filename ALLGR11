    }
    public static boolean distinct(int numb[]){ //A19872013
        boolean distinct = true;
        int length = numb.length;
        for (int i =0; i < 5; i++){
            for (int j = 0; j< 5; j++)
                if (numb[i]==numb[j]&& i!=j){
                    distinct = false;
                }
        }
        return distinct;
    }
    public static int[] turn_2_arr(int num){ //A19872013
        int numberarr[] = new int[5];
        
           numberarr[0]= num/10000;
           numberarr[1]= (num-numberarr[0]*10000)/ 1000;
           numberarr[2]= ( (num-numberarr[1]*1000-numberarr[0]*10000)/ 100);
           numberarr[3]= ( (num-numberarr[2]*100 - numberarr[1]*1000 - numberarr[0]*10000)/ 10);
           numberarr[4]= ( (num-numberarr[3]*10 -numberarr[2]*100 - numberarr[1]*1000- numberarr[0]*10000)/1);
            
       
        return numberarr;
    }
    public static void A19872013(String[] args){
        Scanner input = new Scanner(System.in);
       
        System.out.print("Enter a number: ");
        int years = input.nextInt();
        
        while(!distinct(turn_2_arr(years))){
            years++;
        }
        
        System.out.println("The next int that is distinct is: " + years );
        
              
    }
    public static void HappyorSad(String[] args){
        Scanner input= new Scanner(System.in);
        System.out.println("Enter the string: ");
        String text = input.nextLine();
        int happ =0;
        int sad = 0;
        for (int i = 0; i < text.length(); i++){
            if ((text.charAt(i))==':'){
                if ((text.charAt(i+1))=='-'){
                    if ((text.charAt(i+1))=='('){
                        sad++;
                    }
                    else if ((text.charAt(i+1))==')'){
                        happ++;
                    }
                }
            }
        }
        
        
        if (happ == sad){
            System.out.println("Tie");
        }
        else if  (happ>sad){
            System.out.println("more happ ");
        }
        else {
            System.out.println("more sad ");
        }
    }
